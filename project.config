[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/yadam123
[access "refs/for/refs/*"]
	read = group user/yadam123
	push = group user/yadam123
	label-Code-Review = -2..+2 group user/yadam123
	label-Verified = -1..+1 group user/yadam123
	rebase = group user/yadam123
	removeReviewer = group user/yadam123
	submit = group user/yadam123
[access "refs/heads/*"]
	read = group user/yadam123
	create = group user/yadam123
	push = +force group user/yadam123
	pushMerge = group user/yadam123
[access "refs/tags/*"]
	push = +force group user/yadam123
	pushTag = group user/yadam123
	pushSignedTag = group user/yadam123
